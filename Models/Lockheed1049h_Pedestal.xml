    <type>pick</type>
    <object-name>FlapLever</object-name>
    <action>
      <button>0</button>
      <repeatable>false</repeatable>
      <binding>
        <command>nasal</command>
        <script>controls.flapsDown(1)</script>
      </binding>
      <mod-up>
        <binding>
          <command>nasal</command>
          <script>controls.flapsDown(0)</script>
        </binding>
      </mod-up>
    </action>
    <action>
      <button>1</button>
      <repeatable>false</repeatable>
      <binding>
        <command>nasal</command>
        <script>controls.flapsDown(-1)</script>
      </binding>
      <mod-up>
        <binding>
          <command>nasal</command>
          <script>controls.flapsDown(0)</script>
        </binding>
      </mod-up>
    </action>
  </animation>

