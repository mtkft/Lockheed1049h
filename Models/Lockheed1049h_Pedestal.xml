    <interpolation>
      <entry><ind>   0</ind><dep> 0</dep></entry>
      <entry><ind>0.25</ind><dep>65</dep></entry>
      <entry><ind>0.50</ind><dep>75</dep></entry>
      <entry><ind>0.75</ind><dep>85</dep></entry>
      <entry><ind>   1</ind><dep>95</dep></entry>
    </interpolation>
    <center>
      <x-m>-15.496</x-m>
      <y-m>0.0</y-m>
      <z-m>0.735</z-m>
    </center>
