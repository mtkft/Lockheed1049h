    <object-name>sockle.001</object-name>
    <object-name>switch.000</object-name>
    <object-name>switch.001</object-name>
    <emission>
      <red>0.2</red>
      <green>0.2</green>
      <blue>0.2</blue>
      <factor-prop>/controls/lighting/chart</factor-prop>
    </emission>
  </animation>

  <!-- Panel Glows -->

  <animation>
    <type>material</type>
    <object-name>PedestalShelfText</object-name>  
    <condition>
      <not><property>sim/rendering/rembrandt/enabled</property></not>
    </condition>
    <emission>
      <red>0.7</red>
      <green>0.2</green>
      <blue>0.1</blue>
      <factor-prop>/controls/lighting/panel-norm</factor-prop>
    </emission>
  </animation>

  <animation>
    <type>material</type>
    <object-name>RudderTabCursorFace</object-name>
    <object-name>AileronTabCursorFace</object-name>
    <emission>
      <red>0.4</red>
      <green>0.2</green>
      <blue>0.1</blue>
      <factor-prop>/controls/lighting/panel-norm</factor-prop>
    </emission>
  </animation>


  <animation>
    <type>translate</type>
    <object-name>ParkingBrakeLeverL</object-name>
    <property>controls/gear/brake-parking</property>
    <interpolation>
      <entry><ind>0.0</ind><dep>-0.07</dep></entry>
      <entry><ind>1.0</ind><dep>    0</dep></entry>
    </interpolation>
    <axis>
      <x>1</x>
      <y>0</y>
      <z>0</z>
    </axis>
  </animation>
  <animation>
    <type>translate</type>
    <object-name>ParkingBrakeLeverR</object-name>
    <property>controls/gear/brake-parking</property>
    <interpolation>
      <entry><ind>0.0</ind><dep>-0.07</dep></entry>
      <entry><ind>1.0</ind><dep>    0</dep></entry>
    </interpolation>
    <axis>
      <x>1</x>
      <y>0</y>
      <z>0</z>
    </axis>
  </animation>
  <animation>
    <type>pick</type>
    <object-name>ParkingBrakeLeverL</object-name>  
    <object-name>ParkingBrakeLeverR</object-name>
    <action>
      <button>0</button>
      <binding>
        <command>property-toggle</command>
        <property>controls/gear/brake-parking</property>
      </binding>
    </action>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>RudderTabNeedle</object-name>
    <property>controls/flight/rudder-trim</property>
    <factor>60</factor>
    <center>
      <x-m>-15.263</x-m>
      <y-m>-0.054</y-m>
      <z-m>0.0</z-m>
    </center>
    <axis>
      <x>0.0</x>
      <y>0.0</y>
      <z>-1.0</z>
    </axis>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>AileronTabNeedle</object-name>
    <property>controls/flight/aileron-trim</property>
    <factor>60</factor>
    <center>
      <x-m>0.0</x-m>
      <y-m>-0.054</y-m>
      <z-m>0.596</z-m>
    </center>
    <axis>
      <x>1.0</x>
      <y>0.0</y>
      <z>0.0</z>
    </axis>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>RudderTab</object-name>
    <property>controls/flight/rudder-trim</property>
    <factor>-4680</factor>
    <center>
      <x-m>-15.228</x-m>
      <y-m>-0.055</y-m>
      <z-m>0.0</z-m>
    </center>
    <axis>
      <x>0.0</x>
      <y>0.0</y>
      <z>1.0</z>
    </axis>
  </animation>
  <animation>
    <type>pick</type>
    <object-name>RudderTab</object-name>
    <action>
      <button>0</button>
      <repeatable>true</repeatable>
      <interval-sec>0.2</interval-sec>
      <binding>
        <command>nasal</command>
        <script>Lockheed1049h.applyTrimWheels(0, 1)</script>
      </binding>
    </action>
    <action>
      <button>3</button>
      <repeatable>true</repeatable>
      <interval-sec>0.2</interval-sec>
      <binding>
        <command>property-adjust</command>
        <property>controls/flight/rudder-trim</property>
        <step>0.01</step>
        <min>-1</min>
        <max>1</max>
        <wrap>false</wrap>
      </binding>
    </action>
    <action>
      <button>4</button>
      <repeatable>true</repeatable>
      <interval-sec>0.2</interval-sec>
      <binding>
        <command>property-adjust</command>
        <property>controls/flight/rudder-trim</property>
        <step>-0.01</step>
        <min>-1</min>
        <max>1</max>
        <wrap>false</wrap>
      </binding>
    </action>
  </animation>

