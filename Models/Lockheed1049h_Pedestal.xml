  <animation>
    <type>knob</type>
    <object-name>PropPitch</object-name>
    <property>controls/engines/engine[0]/propeller-pitch</property>
    <factor>-47</factor>
    <offset-deg>47</offset-deg>   
    <center>
      <x-m>-15.496</x-m>
      <y-m>0.0</y-m>
      <z-m>0.735</z-m>
