      <x>0.0</x>
      <y>1.0</y>
      <z>0.0</z>
    </axis>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>ReverseThrottle2</object-name>
    <property>controls/engines/engine[1]/reverser</property>
    <factor>74</factor>
    <center>
      <x-m>-15.37</x-m>
      <y-m>0.0</y-m>
      <z-m>0.85</z-m>
    </center>
    <axis>
      <x>0.0</x>
      <y>1.0</y>
      <z>0.0</z>
    </axis>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>ReverseThrottle3</object-name>
    <property>controls/engines/engine[2]/reverser</property>
    <factor>74</factor>
    <center>
      <x-m>-15.37</x-m>
      <y-m>0.0</y-m>
      <z-m>0.85</z-m>
    </center>
    <axis>
      <x>0.0</x>
      <y>1.0</y>
      <z>0.0</z>
    </axis>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>ReverseThrottle4</object-name>
    <property>controls/engines/engine[3]/reverser</property>
    <factor>74</factor>
    <center>
      <x-m>-15.37</x-m>
      <y-m>0.0</y-m>
      <z-m>0.85</z-m>
    </center>
    <axis>
      <x>0.0</x>
      <y>1.0</y>
      <z>0.0</z>
    </axis>
  </animation>

  <!-- Wiper -->
  <animation>
    <type>knob</type>
    <object-name>KnobPedWW</object-name>
    <property>controls/special/wiper-switch</property>
    <interpolation>
      <entry><ind>0</ind><dep>   0</dep></entry>
      <entry><ind>1</ind><dep>  45</dep></entry>
      <entry><ind>2</ind><dep> 225</dep></entry>
      <entry><ind>3</ind><dep> 270</dep></entry>
      <entry><ind>4</ind><dep> 315</dep></entry>
    </interpolation>
    <center>
      <x-m>-15.356</x-m>
      <y-m> -0.045</y-m>
      <z-m>  0.0</z-m>
    </center>
    <axis>
      <x>0.0</x>
      <y>0.0</y>
      <z>1.0</z>
    </axis>
    <action>
      <binding>
        <command>property-adjust</command>
        <property>controls/special/wiper-switch</property>
        <factor>1</factor>
        <min>0</min>
        <max>5</max>
        <wrap>0</wrap>
      </binding>
    </action>
  </animation>

  <!-- The Autopilot support -->

  <animation>
    <type>knob</type>
    <object-name>switch.001</object-name>
    <property>autopilot/switches/ap</property>
    <factor>-50</factor>
    <center>
      <x-m>-15.292</x-m>
      <y-m>  0.055</y-m>
      <z-m>  0.7</z-m>
    </center>
    <axis>
      <x>0.0</x>
      <y>1.0</y>
      <z>0.0</z>
    </axis>
    <action>
      <binding>
        <command>nasal</command>
        <script>Lockheed1049h.toggle_autopilot_on_off()</script>
      </binding>
    </action>
  </animation>

  <!-- Autopilot ALT on off -->
  <animation>
    <type>knob</type>
    <object-name>switch.000</object-name>
    <property>autopilot/switches/alt</property>
    <factor>-50</factor>
    <center>
      <x-m>-15.292</x-m>
      <y-m>  0.088</y-m>
      <z-m>  0.7</z-m>
    </center>
    <axis>
      <x>0.0</x>
      <y>1.0</y>
      <z>0.0</z>
    </axis>
    <action>
      <binding>
        <command>property-toggle</command>
        <property>autopilot/switches/alt</property>
      </binding>
    </action>
  </animation>

  <!-- Autopilot Pitch wheels -->
  <animation>
    <type>knob</type>
    <object-name>APKnobPitch</object-name>
    <property>autopilot/settings/target-pitch-deg</property>
    <factor>-3</factor>
    <center>
      <x-m>-15.2501</x-m>
      <y-m>  0.0</y-m>
      <z-m>  0.6666</z-m>
    </center>
    <axis>
      <x>0.0</x>
      <y>-1.0</y>
      <z>0.0</z>
    </axis>
    <action>
      <binding>
        <condition>
          <not>
            <property>autopilot/switches/alt</property>
          </not>
        </condition>
        <command>property-adjust</command>
        <property>autopilot/settings/target-pitch-deg</property>
        <factor>0.1</factor>
        <min>-20</min>
        <max>20</max>
        <wrap>0</wrap>
      </binding>
      <binding>
        <condition>
          <and>
            <property>autopilot/switches/alt</property>
            <not>
              <equals>
                <property>sim/messages/pilot</property>
                <value>Pitch wheel is locked when altitude control switch is on</value>
              </equals>
            </not>
          </and>
        </condition>
        <command>property-assign</command>
        <property>sim/messages/pilot</property>
        <value>Pitch wheel is locked when altitude control switch is on</value>
      </binding>
    </action>
  </animation>

  <!-- Autopilot heading control -->
  <animation>
    <type>knob</type>
    <object-name>APKnobHdg</object-name>
    <property>autopilot/settings/target-bank-deg</property>
    <offset-deg>0</offset-deg>
    <factor>4.5</factor>
    <axis>
      <x1-m>-15.226</x1-m>
      <y1-m>0.0727</y1-m>
      <z1-m>0.7252</z1-m>
      <x2-m>-15.2641</x2-m>
      <y2-m>0.0727</y2-m>
      <z2-m>0.6436</z2-m>
    </axis>
    <action>
      <binding>
        <command>property-adjust</command>
        <property>autopilot/settings/target-bank-deg</property>
        <factor>1</factor>
        <min>-30</min>
        <max>30</max>
        <wrap>false</wrap>
      </binding>
    </action>
  </animation>

  <!-- Autopilot select knob -->
  <animation>
    <type>knob</type>
    <object-name>KnobPedFP</object-name>
    <property>autopilot/settings/flight-path</property>
    <interpolation>
      <entry><ind>0</ind><dep>   5</dep></entry>  <!-- OFF (Bank Select) -->
      <entry><ind>1</ind><dep> 330</dep></entry>  <!-- VHF VOR/NDB -->
      <entry><ind>2</ind><dep> 295</dep></entry>  <!-- Localizer -->
      <entry><ind>3</ind><dep> 260</dep></entry>  <!-- Localizer and GS -->
    </interpolation>
    <center>
      <x-m>-15.3567</x-m>
      <y-m>  0.0221</y-m>
      <z-m>  0.0</z-m>
    </center>
    <axis>
      <x>0.0</x>
      <y>0.0</y>
      <z>1.0</z>
    </axis>
    <action>
      <binding>
        <command>property-adjust</command>
        <property>autopilot/settings/flight-path</property>
        <factor>1</factor>
        <min>0</min>
        <max>3</max>
        <wrap>0</wrap>
      </binding>
    </action>
  </animation>
  
</PropertyList>
