<?xml version="1.0"?>

<!-- Lockheed Constellation 1049h        -->
<!-- Gary R. Neely 'Buckaroo', Oct 2010  -->

<PropertyList>

  <path>mesh_props.ac</path>


  <!-- Special Shaders -->

  <animation>
    <type>noshadow</type>
    <object-name>Prop1</object-name>
    <object-name>Prop2</object-name>
    <object-name>Prop3</object-name>
    <object-name>Prop4</object-name>
    <object-name>PropBlurSlow1</object-name>
    <object-name>PropBlurSlow2</object-name>
    <object-name>PropBlurSlow3</object-name>
    <object-name>PropBlurSlow4</object-name>
    <object-name>PropBlurFast1</object-name>
    <object-name>PropBlurFast2</object-name>
    <object-name>PropBlurFast3</object-name>
    <object-name>PropBlurFast4</object-name>
  </animation>


  <!-- Livery machine texture assignments -->

  <animation>
    <type>material</type>
    <object-name>Prop1</object-name>
    <object-name>Prop2</object-name>
    <object-name>Prop3</object-name>
    <object-name>Prop4</object-name>
    <property-base>sim/model/propellers</property-base>
    <texture-prop>texture</texture-prop>
    <texture>txt_livery_prop1.rgb</texture>
  </animation>
  <animation>
    <type>material</type>
    <object-name>PropBlurSlow1</object-name>
    <object-name>PropBlurSlow2</object-name>
    <object-name>PropBlurSlow3</object-name>
    <object-name>PropBlurSlow4</object-name>
    <object-name>PropBlurFast1</object-name>
    <object-name>PropBlurFast2</object-name>
    <object-name>PropBlurFast3</object-name>
    <object-name>PropBlurFast4</object-name>
    <property-base>sim/model/propellerdisks</property-base>
    <texture-prop>texture</texture-prop>
    <texture>txt_propblur.rgb</texture>
  </animation>

  <!-- working for Rembrandt -->
  <effect>
    <inherits-from>Effects/model-transparent</inherits-from>
    <object-name>PropBlurSlow1</object-name>
    <object-name>PropBlurSlow2</object-name>
    <object-name>PropBlurSlow3</object-name>
    <object-name>PropBlurSlow4</object-name>
    <object-name>PropBlurFast1</object-name>
    <object-name>PropBlurFast2</object-name>
    <object-name>PropBlurFast3</object-name>
    <object-name>PropBlurFast4</object-name>
  </effect>

  <!-- Animations  -->

  <!-- Selects: Prop Models -->

  <animation>
    <type>select</type>
    <object-name>Prop1</object-name>
    <condition>
      <or>
        <less-than>
          <property>engines/engine[0]/rpm</property>
          <value>500</value>
        </less-than>
        <and>
          <greater-than-equals>
            <property>engines/engine[0]/rpm</property>
            <value>500</value>
          </greater-than-equals>
          <not><property>engines/engine[0]/running</property></not>
        </and>          
      </or>
    </condition>
  </animation>
  <animation>
    <type>select</type>
    <object-name>Prop2</object-name>
    <condition>
      <or>
        <less-than>
          <property>engines/engine[1]/rpm</property>
          <value>500</value>
        </less-than>
        <and>
          <greater-than-equals>
            <property>engines/engine[1]/rpm</property>
            <value>500</value>
          </greater-than-equals>
          <not><property>engines/engine[1]/running</property></not>
        </and>          
      </or>
    </condition>
  </animation>
  <animation>
    <type>select</type>
    <object-name>Prop3</object-name>
