<?xml version="1.0"?>

<!-- Lockheed Constellation 1049h        -->
<!-- Gary R. Neely 'Buckaroo', Oct 2010  -->


<PropertyList>

  <path>mesh_airframe.ac</path>

  <!-- Start livery machine -->
  <nasal>
    <load><![CDATA[
      var remote_player_node = cmdarg();
      var livery_update = aircraft.livery_update.new("Aircraft/Lockheed1049h/Models/Liveries");
      remote_player_node.getNode ("controls/lighting/cabin", 1)
        .alias (remote_player_node.getNode ("sim/multiplay/generic/float[0]"));
      remote_player_node.getNode ("controls/door-crew/position-norm", 1)
        .alias (remote_player_node.getNode ("sim/multiplay/generic/float[4]"));
      remote_player_node.getNode ("position/altitude-agl-ft", 1)
        .alias (remote_player_node.getNode ("sim/multiplay/generic/float[5]"));
      io.load_nasal (getprop ("/sim/fg-root") ~ "/Aircraft/Generic/DualControl/dual-control-tools.nas", "dual_control_tools");
      # This decoder must be symmetric with the encoder in Nasal/Lockheed1049h.nas.
      var decoder = dual_control_tools.SwitchDecoder.new (
        remote_player_node.getNode ("sim/multiplay/generic/int[1]", 1),
        [
          func (b) { remote_player_node.getNode ("controls/fuel/jettison[0]/spray").setBoolValue (b); },
          func (b) { remote_player_node.getNode ("controls/fuel/jettison[1]/spray").setBoolValue (b); },
          func (b) { remote_player_node.getNode ("controls/lighting/beacon/enabled").setBoolValue (b); },
          func (b) { remote_player_node.getNode ("controls/lighting/beacon/state").setBoolValue (b); },
          func (b) { remote_player_node.getNode ("controls/lighting/landing-extend-left").setBoolValue (b); },
          func (b) { remote_player_node.getNode ("controls/lighting/landing-extend-right").setBoolValue (b); },
          func (b) { remote_player_node.getNode ("controls/lighting/landing-left").setBoolValue (b); },
          func (b) { remote_player_node.getNode ("controls/lighting/landing-right").setBoolValue (b); },
          func (b) { remote_player_node.getNode ("controls/lighting/nav").setBoolValue (b); },
          func (b) { remote_player_node.getNode ("controls/lighting/tail").setBoolValue (b); },
          func (b) { remote_player_node.getNode ("controls/lighting/taxi").setBoolValue (b); },
          func (b) { remote_player_node.getNode ("engines/engine[0]/running").setBoolValue (b); },
          func (b) { remote_player_node.getNode ("engines/engine[1]/running").setBoolValue (b); },
          func (b) { remote_player_node.getNode ("engines/engine[2]/running").setBoolValue (b); },
          func (b) { remote_player_node.getNode ("engines/engine[3]/running").setBoolValue (b); },
          func (b) { remote_player_node.getNode ("hazards/fire/engine[0]").setBoolValue (b); },
          func (b) { remote_player_node.getNode ("hazards/fire/engine[1]").setBoolValue (b); },
          func (b) { remote_player_node.getNode ("hazards/fire/engine[2]").setBoolValue (b); },
          func (b) { remote_player_node.getNode ("hazards/fire/engine[3]").setBoolValue (b); },
        ]);
      var decode_loop = maketimer (0.1, func () { decoder.update (); });
      decode_loop.start ();
    ]]></load>
    <unload><![CDATA[
      livery_update.stop();
      decode_loop.stop ();
    ]]></unload>
  </nasal>

