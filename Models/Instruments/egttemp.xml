<?xml version="1.0"?>

<!--
Instrument displays degC, so these conversions are used:
degC	degF
200     392
900     1652
-->

<PropertyList>

  <path>egt.ac</path>

  <animation>
    <type>material</type>
    <object-name>EGTTemp.FaceBack</object-name>
    <object-name>EGTTemp.Needle1</object-name>
    <object-name>EGTTemp.Needle2</object-name>
    <emission>
      <red>0.8</red>
      <green>0.4</green>
      <blue>0.2</blue>
      <factor-prop>controls/lighting/panel-norm</factor-prop>
    </emission>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>EGTTemp.Needle1</object-name>
    <property alias="../../egt1-degf"/>
    <interpolation>
      <entry><ind> 392</ind><dep> 52.5</dep></entry>
      <entry><ind>1652</ind><dep>-52.5</dep></entry>
    </interpolation>
    <center>
      <x-m>-0.0251</x-m>
      <y-m>0</y-m>
      <z-m>0</z-m>
    </center>
    <axis>
      <x>0</x>
      <y>1</y>
      <z>0</z>
    </axis>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>EGTTemp.Needle2</object-name>
    <property alias="../../egt2-degf"/>
    <interpolation>
      <entry><ind> 392</ind><dep> 52.5</dep></entry>
      <entry><ind>1652</ind><dep>-52.5</dep></entry>
    </interpolation>
    <center>
      <x-m>0.0251</x-m>
      <y-m>0</y-m>
      <z-m>0</z-m>
    </center>
    <axis>
      <x>0</x>
      <y>-1</y>
      <z>0</z>
    </axis>
  </animation>

</PropertyList>
