<?xml version="1.0"?>

<PropertyList>

  <path>swplate_gpsslave.ac</path>

  <animation>
    <type>material</type>
    <object-name>PlacardAlpha</object-name>
    <emission>
      <red>0.7</red>
      <green>0.2</green>
      <blue>0.1</blue>
      <factor-prop>/controls/lighting/panel-norm</factor-prop>
    </emission>
  </animation>

  <animation>
    <type>material</type>
    <object-name>SwSlaveGPS</object-name>
    <emission>
      <red>0.05</red>
      <green>0.025</green>
      <blue>0.0125</blue>
      <factor-prop>/controls/lighting/panel-norm</factor-prop>
    </emission>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>SwSlaveGPS</object-name>
    <property>instrumentation/nav[0]/slaved-to-gps</property>
    <interpolation>
      <entry><ind>0</ind><dep>-25</dep></entry>
      <entry><ind>1</ind><dep> 25</dep></entry>
    </interpolation>
    <center>
      <x-m>-0.002</x-m>
      <y-m>0.0</y-m>
      <z-m>0.023</z-m>
    </center>
    <axis>
      <x>0.0</x>
      <y>1.0</y>
      <z>0.0</z>
    </axis>
  </animation>

  <animation>
    <type>pick</type>
    <object-name>SwSlaveGPS</object-name>
    <action>
      <button>0</button>
      <binding>
        <command>property-toggle</command>
        <property>instrumentation/nav[0]/slaved-to-gps</property>
      </binding>
    </action>
  </animation>
</PropertyList>
