<?xml version="1.0"?>

<PropertyList>

  <path>prop_gov.ac</path>

  <animation>
    <type>select</type>
    <object-name>LampPropGovOn1</object-name>
    <condition>
      <or>
        <greater-than-equals>
          <property>engines/engine[0]/rpm</property>
          <value>2900.0</value>
