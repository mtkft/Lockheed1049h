<!-- Lockheed Constellation 1049 fuzzy dice -->
<!-- Gary R. Neely 'Buckaroo', June 2008    -->


<PropertyList>

  <path>fuzzy_dice.ac</path>

  <animation>
    <type>select</type>
    <object-name>Dice</object-name>
    <condition>
      <not>
        <property>sim/model/options/hide-dice</property>
      </not>
    </condition>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>Dice</object-name>
    <property>orientation/roll-deg</property>
    <interpolation>
      <entry><ind>-90.0</ind><dep>-90.0</dep></entry>
      <entry><ind> 90.0</ind><dep> 90.0</dep></entry>
    </interpolation>
    <axis>
      <x>1</x>
      <y>0</y>
      <z>0</z>
    </axis>
    <center>
      <x-m>0</x-m>
      <y-m>0</y-m>
      <z-m>0</z-m>
    </center>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>Dice</object-name>
    <property>orientation/pitch-deg</property>
    <interpolation>
      <entry><ind>-90.0</ind><dep>-90.0</dep></entry>
      <entry><ind> 90.0</ind><dep> 90.0</dep></entry>
    </interpolation>
    <axis>
      <x>0</x>
      <y>-1</y>
      <z>0</z>
    </axis>
    <center>
      <x-m>0</x-m>
      <y-m>0</y-m>
      <z-m>0</z-m>
    </center>
  </animation>

  <animation>
    <type>noshadow</type>
    <object-name>Dice</object-name>
  </animation>

</PropertyList>
