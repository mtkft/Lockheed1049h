<?xml version="1.0"?>

<PropertyList>

  <animation>
    <type>material</type>
    <object-name>ClockEng.Face</object-name>
    <object-name>ClockEng.Hour</object-name>
    <object-name>ClockEng.Minute</object-name>
    <object-name>ClockEng.Second</object-name>
    <emission>
      <red>0.8</red>
      <green>0.4</green>
      <blue>0.2</blue>
      <factor-prop>/controls/lighting/panel-norm</factor-prop>
    </emission>
  </animation>

  <path>clock_engineer.ac</path>

  <animation>
    <type>rotate</type>
    <object-name>ClockEng.Hour</object-name>
    <property>instrumentation/clock/indicated-sec</property>
    <factor>0.00833333333333333</factor>
    <axis>
      <x>0</x>
      <y>0.574</y>
      <z>0.819</z>
    </axis>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>ClockEng.Minute</object-name>
    <property>instrumentation/clock/indicated-sec</property>
    <factor>0.1</factor>
    <axis>
      <x>0</x>
      <y>0.574</y>
      <z>0.819</z>
    </axis>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>ClockEng.Second</object-name>
    <property>instrumentation/clock/indicated-sec</property>
    <factor>6</factor>
    <axis>
      <x>0</x>
      <y>0.574</y>
      <z>0.819</z>
    </axis>
  </animation>

</PropertyList>

