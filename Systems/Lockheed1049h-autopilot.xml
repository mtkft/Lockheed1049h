<?xml version="1.0"?>

<!-- Lockheed 1049h Custom Autopilot -->
<!-- Copyright (c) 2021 Josh Davidson (Octal450) -->

<PropertyList>
	
	<!-- Drivers -->
	<filter>
		<name>Heading Error Deg</name>
		<debug>false</debug>
		<type>gain</type>
		<gain>1.0</gain>
		<input>
			<condition>
				<equals>
					<property>/autopilot/locks/heading</property>
					<value>nav1-hold</value>
				</equals>
			</condition>
			<property>/instrumentation/navx/radials/target-auto-hdg-deg</property>
			<offset>
				<property>/orientation/heading-deg</property>
				<scale>-1.0</scale>
			</offset>
		</input>
		<input>
			<property>/autopilot/settings/heading-bug-deg</property>
			<offset>
				<property>/instrumentation/heading-indicator/indicated-heading-deg</property>
				<scale>-1.0</scale>
			</offset>
		</input>
		<output>/autopilot/internal/heading-error-deg</output>
		<period>
			<min>-180</min>
			<max>180</max>
		</period>
	</filter>
	
	<filter>
		<name>Nav Course Error</name>
		<type>gain</type>
		<gain>1.0</gain>
		<input>
			<property>/instrumentation/navx/radials/selected-deg</property>
			<offset>
				<property>/instrumentation/heading-indicator/indicated-heading-deg</property>
				<scale>-1.0</scale>
			</offset>
		</input>
		<output>/autopilot/internal/nav-course-error</output>
		<period>
			<min>-180</min>
			<max>180</max>
		</period>
	</filter>
	
	<filter>
		<name>Internal Vertical Speed</name>
		<debug>false</debug>
		<type>gain</type>
		<gain>1.0</gain>
		<input>/instrumentation/gps/indicated-vertical-speed</input>
		<output>/autopilot/internal/vert-speed-fpm</output>
	</filter>
	
	<filter>
		<name>Vertical Speed Filter</name>
		<debug>false</debug>
		<type>noise-spike</type>
		<feedback-if-disabled>true</feedback-if-disabled>
		<initialize-to>output</initialize-to>
		<input>/autopilot/settings/vertical-speed-fpm</input>
		<output>/autopilot/internal/vertical-speed-fpm</output>
		<max-rate-of-change>
			<condition>
				<equals>
					<property>/autopilot/locks/altitude</property>
					<value>vertical-speed-hold</value>
				</equals>
			</condition>
			<value>300</value>
		</max-rate-of-change>
		<max-rate-of-change>2000</max-rate-of-change>
	</filter>
	
	<filter>
		<name>Nav G/S FPM Calc</name>
		<debug>false</debug>
		<type>gain</type>
		<gain>1.0</gain>
		<input>/instrumentation/navx/gs-rate-of-climb-fpm</input>
		<output>/autopilot/internal/nav-rate-of-climb-fpm</output>
		<min>-1200</min>
		<max>0</max>
	</filter>
	
	<flipflop> <!-- Author: Richard Senior -->
		<name>Wing Leveler Heading Hold</name>
		<debug>false</debug>
		<type>SR</type>
		<S>
			<and>
				<equals>
					<property>/autopilot/locks/heading</property>
					<value>wing-leveler</value>
				</equals>
				<less-than>
					<expression>
						<abs><property>/autopilot/settings/target-bank-deg</property></abs>
					</expression>
					<value>1.0</value>
				</less-than>
				<less-than>
					<expression>
						<abs><property>/orientation/roll-deg</property></abs>
					</expression>
					<value>1.0</value>
				</less-than>
			</and>
		</S>
		<R>
			<or>
				<not-equals>
					<property>/autopilot/locks/heading</property>
					<value>wing-leveler</value>
				</not-equals>
				<greater-than-equals>
					<expression>
						<abs><property>/autopilot/settings/target-bank-deg</property></abs>
					</expression>
					<value>1.0</value>
				</greater-than-equals>
			</or>
		</R>
		<output>/autopilot/internal/wing-leveler-heading-hold</output>
	</flipflop>
	
	<flipflop> <!-- Author: Richard Senior -->
		<name>Glideslope Capture</name>
		<type>SR</type>
		<debug>false</debug>
		<S>
			<and>
				<equals>
					<property>/autopilot/locks/altitude</property>
					<value>gs1-hold</value>
				</equals>
				<property>/instrumentation/navx/gs-in-range</property>
				<less-than-equals> <!-- Modified by Josh, smoother capture -->
					<property>/instrumentation/navx/gs-needle-deflection</property>
					<value>0.2</value>
				</less-than-equals>
			</and>
		</S>
		<R>
			<or>
				<not-equals>
					<property>/autopilot/locks/altitude</property>
					<value>gs1-hold</value>
				</not-equals>
				<not><property>/instrumentation/navx/gs-in-range</property></not>
			</or>
		</R>
		<output>/autopilot/internal/gs-captured</output>
	</flipflop>
	
	<!-- Roll Axis -->
	<filter>
		<name>IT-CONTROLLER: Bank Angle Select</name>
		<debug>false</debug>
		<type>gain</type>
		<gain>1.0</gain>
		<enable>
			<condition>
				<and>
					<equals>
						<property>/autopilot/locks/heading</property>
						<value>wing-leveler</value>
					</equals>
					<not><property>/autopilot/internal/wing-leveler-heading-hold</property></not>
				</and>
			</condition>
		</enable>
		<input>/autopilot/settings/target-bank-deg</input>
		<output>/autopilot/internal/target-roll-deg</output>
	</filter>
	
	<filter>
		<name>IT-CONTROLLER: Heading/VOR/LOC</name>
		<debug>false</debug>
		<type>gain</type>
		<gain>
			<expression>
				<table>
					<property>/velocities/airspeed-kt</property>
					<entry><ind>130</ind><dep>0.8</dep></entry>
					<entry><ind>300</ind><dep>2.5</dep></entry>
				</table>
			</expression>
		</gain>
		<enable>
			<condition>
				<or>
					<equals>
						<property>/autopilot/locks/heading</property>
						<value>dg-heading-hold</value>
					</equals>
					<property>/autopilot/internal/wing-leveler-heading-hold</property>
					<equals>
						<property>/autopilot/locks/heading</property>
						<value>nav1-hold</value>
					</equals>
				</or>
			</condition>
		</enable>
		<input>/autopilot/internal/heading-error-deg</input>
		<reference>0</reference>
		<output>/autopilot/internal/target-roll-deg</output>
		<min>
			<property>/controls/autoflight/bank-angle-select</property>
			<scale>-1.0</scale>
		</min>
		<max>/controls/autoflight/bank-angle-select</max>
	</filter>
	
	<filter>
		<name>System Command: Roll Target Inactive Sync</name>
		<debug>false</debug>
		<type>gain</type>
		<gain>1.0</gain>
		<enable>
			<condition>
				<equals>
					<property>autopilot/locks/heading</property>
					<value></value>
				</equals>
			</condition>
		</enable>
		<input>/orientation/roll-deg</input>
		<output>/autopilot/internal/target-roll-deg</output>
		<min>
			<property>/controls/autoflight/bank-angle-select</property>
			<scale>-1.0</scale>
		</min>
		<max>/controls/autoflight/bank-angle-select</max>
	</filter>
	
	<filter>
		<name>System Command: Target Roll Filter</name>
		<debug>false</debug>
		<type>noise-spike</type>
		<input>/autopilot/internal/target-roll-deg</input>
		<output>/autopilot/internal/target-roll</output>
		<max-rate-of-change>
			<condition>
				<not-equals>
					<property>autopilot/locks/heading</property>
					<value></value>
				</not-equals>
			</condition>
			<value>6</value>
		</max-rate-of-change>
		<max-rate-of-change>50</max-rate-of-change>
	</filter>
	
	<filter>
		<name>System Command: Roll Rate Target</name>
		<debug>false</debug>
		<type>gain</type>
		<gain>-0.6</gain>
		<input>/orientation/roll-deg</input>
		<reference>/autopilot/internal/target-roll</reference>
		<output>/autopilot/internal/roll-rate</output>
		<min>-4</min>
		<max>4</max>
	</filter>
	
	<pid-controller>
		<name>System Command: Aileron</name>
		<debug>false</debug>
		<enable>
			<condition>
				<not-equals>
					<property>autopilot/locks/heading</property>
					<value></value>
				</not-equals>
			</condition>
		</enable>
		<input>/orientation/roll-rate-degps</input>
		<reference>/autopilot/internal/roll-rate</reference>
		<output>/autopilot/internal/aileron-cmd</output>
		<config>
			<Kp>
				<expression>
					<table>
						<property>/velocities/airspeed-kt</property>
						<entry><ind>130</ind><dep>0.08</dep></entry>
						<entry><ind>300</ind><dep>0.02</dep></entry>
					</table>
				</expression>
			</Kp>
			<Ti>1.0</Ti>
			<Td>0.0001</Td>
			<u_min>-1.0</u_min>
			<u_max>1.0</u_max>
		</config>
	</pid-controller>
	
	<filter>
		<name>System Command: Aileron Filter</name>
		<debug>false</debug>
		<type>noise-spike</type>
		<feedback-if-disabled>true</feedback-if-disabled>
		<initialize-to>output</initialize-to>
		<enable>
			<condition>
				<not-equals>
					<property>autopilot/locks/heading</property>
					<value></value>
				</not-equals>
			</condition>
		</enable>
		<input>/autopilot/internal/aileron-cmd</input>
		<output>/controls/flight/aileron</output>
		<max-rate-of-change>1.2</max-rate-of-change>
	</filter>
	
	<!-- Pitch Axis -->
	<filter>
		<name>IT-CONTROLLER: Pitch Angle Select</name>
		<debug>false</debug>
		<type>gain</type>
		<gain>1.0</gain>
		<enable>
			<condition>
				<equals>
					<property>/autopilot/locks/altitude</property>
					<value>pitch-hold</value>
				</equals>
			</condition>
		</enable>
		<input>/autopilot/settings/target-pitch-deg</input>
		<output>/autopilot/internal/target-pitch-deg</output>
	</filter>
	
	<filter>
		<name>IT-CONTROLLER: Altitude Capture/Hold</name>
		<debug>false</debug>
		<type>gain</type>
		<gain>-5</gain>
		<input>
			<expression>
				<sum>
					<property>/instrumentation/altimeter/indicated-altitude-ft</property>
					<value>-2</value>
				</sum>
			</expression>
		</input>
		<reference>/autopilot/settings/target-altitude-ft</reference>
		<output>/autopilot/internal/target-fpm-alt</output>
		<min>-500</min>
		<max>500</max>
	</filter>
	
	<pid-controller>
		<name>IT-CONTROLLER: FPM Hold</name>
		<debug>false</debug>
		<enable>
			<condition>
				<or>
					<equals>
						<property>/autopilot/locks/altitude</property>
						<value>altitude-hold</value>
					</equals>
					<equals>
						<property>/autopilot/locks/altitude</property>
						<value>gs1-hold</value>
					</equals>
					<equals>
						<property>/autopilot/locks/altitude</property>
						<value>vertical-speed-hold</value>
					</equals>
				</or>
			</condition>
		</enable>
		<input>/autopilot/internal/vert-speed-fpm</input>
		<reference>
			<condition>
				<and>
					<equals>
						<property>/autopilot/locks/altitude</property>
						<value>gs1-hold</value>
					</equals>
					<property>/autopilot/internal/gs-captured</property>
				</and>
			</condition>
			<property>/autopilot/internal/nav-rate-of-climb-fpm</property>
		</reference>
		<reference>
			<condition>
				<or>
					<equals>
						<property>/autopilot/locks/altitude</property>
						<value>altitude-hold</value>
					</equals>
					<and>
						<equals>
							<property>/autopilot/locks/altitude</property>
							<value>gs1-hold</value>
						</equals>
						<not><property>/autopilot/internal/gs-captured</property></not>
					</and>
				</or>
			</condition>
			<property>/autopilot/internal/target-fpm-alt</property>
		</reference>
		<reference>/autopilot/internal/vertical-speed-fpm</reference>
		<output>/autopilot/internal/target-pitch-deg</output>
		<config>
			<Kp>
				<expression>
					<table>
						<property>/velocities/airspeed-kt</property>
						<entry><ind>140</ind><dep>0.0029</dep></entry>
						<entry><ind>360</ind><dep>0.0009</dep></entry>
					</table>
				</expression>
			</Kp>
			<Ti>2.5</Ti>
			<Td>0.0001</Td>
			<u_min>
				<expression>
					<max>
						<dif>
							<property>/orientation/pitch-deg</property>
							<value>5</value>
						</dif>
						<value>-10</value>
					</max>
				</expression>
			</u_min>
			<u_max>
				<expression>
					<min>
						<sum>
							<property>/orientation/pitch-deg</property>
							<value>5</value>
						</sum>
						<value>20</value>
					</min>
				</expression>
			</u_max>
		</config>
	</pid-controller>
	
	<filter>
		<name>System Command: Pitch Target Inactive Sync</name>
		<debug>false</debug>
		<type>gain</type>
		<gain>1.0</gain>
		<enable>
			<condition>
				<equals>
					<property>autopilot/locks/altitude</property>
					<value></value>
				</equals>
			</condition>
		</enable>
		<input>/orientation/pitch-deg</input>
		<output>/autopilot/internal/target-pitch-deg</output>
		<min>-10</min>
		<max>20</max>
	</filter>
	
	<filter>
		<name>System Command: Pitch Rate Target</name>
		<debug>false</debug>
		<type>gain</type>
		<gain>-0.4</gain>
		<input>/orientation/pitch-deg</input>
		<reference>/autopilot/internal/target-pitch-deg</reference>
		<output>/autopilot/internal/pitch-rate</output>
		<min>-1.25</min>
		<max>1.25</max>
	</filter>
	
	<pid-controller>
		<name>System Command: Elevator</name>
		<debug>false</debug>
		<enable>
			<condition>
				<not-equals>
					<property>autopilot/locks/altitude</property>
					<value></value>
				</not-equals>
			</condition>
		</enable>
		<input>/orientation/pitch-rate-degps</input>
		<reference>/autopilot/internal/pitch-rate</reference>
		<output>/autopilot/internal/elevator-cmd</output>
		<config>
			<Kp>
				<expression>
					<table>
						<property>/velocities/airspeed-kt</property>
						<entry><ind>130</ind><dep>-0.16</dep></entry>
						<entry><ind>300</ind><dep>-0.04</dep></entry>
					</table>
				</expression>
			</Kp>
			<Ti>0.5</Ti>
			<Td>0.001</Td>
			<u_min>-1.0</u_min>
			<u_max>1.0</u_max>
		</config>
	</pid-controller>
	
	<filter>
		<name>System Command: Elevator Filter</name>
		<debug>false</debug>
		<type>noise-spike</type>
		<feedback-if-disabled>true</feedback-if-disabled>
		<initialize-to>output</initialize-to>
		<enable>
			<condition>
				<not-equals>
					<property>autopilot/locks/altitude</property>
					<value></value>
				</not-equals>
			</condition>
		</enable>
		<input>/autopilot/internal/elevator-cmd</input>
		<output>/controls/flight/elevator</output>
		<max-rate-of-change>1.2</max-rate-of-change>
	</filter>
	
	<filter>
		<name>System Command: Elevator Trim</name>
		<debug>false</debug>
		<type>noise-spike</type>
		<feedback-if-disabled>true</feedback-if-disabled>
		<initialize-to>output</initialize-to>
		<enable>
			<condition>
				<and>
					<not-equals>
						<property>autopilot/locks/altitude</property>
						<value></value>
					</not-equals>
					<or>
						<greater-than>
							<property>/autopilot/internal/elevator-cmd</property>
							<value>0.02</value>
						</greater-than>
						<less-than>
							<property>/autopilot/internal/elevator-cmd</property>
							<value>-0.02</value>
						</less-than>
					</or>
				</and>
			</condition>
		</enable>
		<input>
			<condition>
				<greater-than>
					<property>/autopilot/internal/elevator-cmd</property>
					<value>0.02</value>
				</greater-than>
			</condition>
			<value>1</value>
		</input>
		<input>
			<condition>
				<less-than>
					<property>/autopilot/internal/elevator-cmd</property>
					<value>-0.02</value>
				</less-than>
			</condition>
			<value>-1</value>
		</input>
		<output>/controls/flight/elevator-trim</output>
		<min>-1.0</min>
		<max>1.0</max>
		<max-rate-of-change>
			<expression>
				<table>
					<abs><property>/autopilot/internal/elevator-cmd</property></abs>
					<entry><ind>0.02</ind><dep>0.001</dep></entry>
					<entry><ind>0.10</ind><dep>0.010</dep></entry>
				</table>
			</expression>
		</max-rate-of-change>
	</filter>
	
	<!-- Auto Engineer Throttle Controls -->
	<logic>
		<input>
			<and>
				<property>/autoengineer/allow-speedbythrottle</property>
				<equals>
					<property>/autoengineer/holdmp</property>
					<value>0</value>
				</equals>
			</and>
		</input>
		<output>/autopilot/internal/speed-on-throttle-active</output>
	</logic>
	
	<pid-controller>
		<name>IT-CONTROLLER: Speed On Throttle</name>
		<debug>false</debug>
		<enable>
			<condition>
				<property>/autopilot/internal/speed-on-throttle-active</property>
			</condition>
		</enable>
		<input>/instrumentation/airspeed-indicator/indicated-speed-kt</input>
		<reference>/autopilot/settings/target-speed-kt</reference>
		<output>/autopilot/internal/target-mp-inhg</output>
		<config>
			<Kp>3.5</Kp>
			<Ti>7.5</Ti>
			<Td>0.001</Td>
			<u_min>10</u_min>
			<u_max>65</u_max>
		</config>
	</pid-controller>
	
	<filter>
		<name>System Command: Manifold Pressure Target Inactive Sync</name>
		<debug>false</debug>
		<type>gain</type>
		<gain>1.0</gain>
		<enable>
			<condition>
				<not><property>/autopilot/internal/speed-on-throttle-active</property></not>
			</condition>
		</enable>
		<input>
			<expression>
				<max>
					<property>/engines/engine[0]/mp-inhg</property>
					<property>/engines/engine[1]/mp-inhg</property>
					<property>/engines/engine[2]/mp-inhg</property>
					<property>/engines/engine[3]/mp-inhg</property>
				</max>
			</expression>
		</input>
		<output>/autopilot/internal/target-mp-inhg</output>
		<min>10</min>
		<max>65</max>
	</filter>
	
	<logic>
		<input>
			<or>
				<greater-than>
					<property>/autoengineer/holdmp</property>
					<value>0</value>
				</greater-than>
				<property>/autoengineer/allow-speedbythrottle</property>
			</or>
		</input>
		<output>/autopilot/internal/throttle-active</output>
	</logic>
	
	<pid-controller>
		<name>IT-CONTROLLER: Manifold Pressure 1</name>
		<debug>false</debug>
		<enable>
			<condition>
				<property>/autopilot/internal/throttle-active</property>
			</condition>
		</enable>
		<input>/engines/engine[0]/mp-inhg</input>
		<reference>
			<condition>
				<property>/autopilot/internal/speed-on-throttle-active</property>
			</condition>
			<property>/autopilot/internal/target-mp-inhg</property>
		</reference>
		<reference>/autoengineer/holdmp</reference>
		<output>/autopilot/internal/throttle-cmd[0]</output>
		<config>
			<Kp>0.01</Kp>
			<Ti>1.5</Ti>
			<Td>0.01</Td>
			<u_min>0.0</u_min>
			<u_max>1.0</u_max>
		</config>
	</pid-controller>
	
	<pid-controller>
		<name>IT-CONTROLLER: Manifold Pressure 2</name>
		<debug>false</debug>
		<enable>
			<condition>
				<property>/autopilot/internal/throttle-active</property>
			</condition>
		</enable>
		<input>/engines/engine[1]/mp-inhg</input>
		<reference>
			<condition>
				<property>/autopilot/internal/speed-on-throttle-active</property>
			</condition>
			<property>/autopilot/internal/target-mp-inhg</property>
		</reference>
		<reference>/autoengineer/holdmp</reference>
		<output>/autopilot/internal/throttle-cmd[1]</output>
		<config>
			<Kp>0.01</Kp>
			<Ti>1.5</Ti>
			<Td>0.01</Td>
			<u_min>0.0</u_min>
			<u_max>1.0</u_max>
		</config>
	</pid-controller>
	
	<pid-controller>
		<name>IT-CONTROLLER: Manifold Pressure 3</name>
		<debug>false</debug>
		<enable>
			<condition>
